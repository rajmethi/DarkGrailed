{
  "manifest_version": 3,
  "name": "Dark Grailed",
  "version": "1.0",
  "description": "Injects dark mode styles into Grailed.com",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.grailed.com/*"],
      "js": ["content.js"],
      "css": ["dark-mode.css"]
    }
  ]
}